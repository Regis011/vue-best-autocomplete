{
  "name": "vue-best-autocomplete",
  "version": "0.2.4",
  "description": "Simple Vue Autocomplete: Simplify user input with dynamic suggestions. Effortlessly integrate autocomplete functionality into your Vue.js projects.",
  "keywords": [
    "vue",
    "autocomplete",
    "input",
    "component",
    "vue3",
    "vue3-autocomplete",
    "vue3-input",
    "simple autocomplete"
  ],
  "main": "./dist/Autocomplete.common.js",
  "files": [
    "dist"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Regis011/vue-best-autocomplete"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name Autocomplete ./src/components/autocomplete/input.vue",
    "lint": "vue-cli-service lint",
    "publish:lib": "npm run build:lib && npm publish --access public"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "node-sass": "^9.0.0",
    "vue": "^3.2.13",
    "vue-loader": "^17.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass-loader": "^14.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
